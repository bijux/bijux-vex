            "application/json": {
              "examples": {
                "execute": {
                  "value": {
                    "artifact_id": "art-1",
                    "execution_contract": "deterministic",
                    "execution_intent": "exact_validation",
                    "execution_mode": "strict",
                    "top_k": 3,
                    "vector": [
                      0.0,
                      1.0,
                      0.0
                    ]
                  }
                }
              },
              "schema": {
                "$ref": "#/components/schemas/ExecutionRequestPayload"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "examples": {
                  "execute": {
                    "value": {
                      "correlation_id": "req-1",
                      "execution_contract": "deterministic",
                      "execution_contract_status": "stable",
                      "execution_id": "exec-1",
                      "replayable": true,
                      "results": [
